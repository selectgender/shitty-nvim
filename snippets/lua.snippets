snippet r
	return 
snippet q
	require("$1")$0
snippet l
	local 
snippet l=
	local ${1:name} = ${0:value}
snippet f
	function(${1:params})
		$0
	end
snippet lf
	local function ${1:name}(${2:params})
		$0
	end
snippet i 
	if ${1:true} then
		$0
	end
snippet fi 
	for ${1:i} = ${2:1}, ${3:10}, ${4:1} do
		$0
	end
snippet p 
	for ${1:_}, ${2:value} in pairs(${3:table}) do
		$0
	end
snippet ip 
	for ${1:_}, ${2:value} in ipairs(${3:table}) do
		$0
	end
